<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="album"
            type="pro.branium.core_ui.model.DisplayAlbumModel" />
    </data>

    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="pro.branium.feature_album.ui.fragment.AlbumDetailsFragment">

        <include
            android:id="@+id/include_album_detail"
            layout="@layout/partial_playlist"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:album="@{album}"
            app:playlist="@{null}" />
    </FrameLayout>
</layout>
